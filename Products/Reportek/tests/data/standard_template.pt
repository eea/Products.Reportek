<html metal:define-macro="page"
      xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">

<tal:block define="header python:request.RESPONSE.setHeader('content-type', 'text/html;;charset=utf-8')" />
<head>
    <!--[if lt IE 7]>
      </base>
    <![endif]-->

  <meta http-equiv="content-type"
        tal:attributes="content string:text/html;; charset=${here/management_page_charset};" />

  <title tal:content="here/title_or_id"></title>

  <meta name="description"
        tal:condition="here/description | nothing"
        tal:attributes="content here/description" />

  <script type="text/javascript" src="/styles/pageops.js"></script>

  <link rel="stylesheet" type="text/css" media="print" href="http://www.eionet.europa.eu/styles/eionet2007/print.css" />

  <link rel="stylesheet" type="text/css" href="http://www.eionet.europa.eu/styles/eionet2007/screen.css" media="screen" title="Eionet 2007 style" />

  <link rel="stylesheet" type="text/css" media="handheld" href="http://www.eionet.europa.eu/styles/eionet2007/handheld.css" />

  <link rel="stylesheet" type="text/css" href="/styles/eionet2007.css" media="screen" title="Eionet 2007 style" />

  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />

  <tal:block condition="python:here.absolute_url(1) == ''">

  <link rel="alternate" type="application/rss+xml" title="Last 15 deliveries in RSS" href="/last15.rss" />

  <link rel="alternate" type="application/rdf+xml" title="All released envelopes" href="/envelopes.rdf" />

  </tal:block>

  <style type="text/css"
         tal:content="structure here/local_stylesheet | nothing"></style>

  <metal:block define-slot="head"></metal:block>
</head>

<body>
  <div id="container">
    <div id="toolribbon">

      <div id="lefttools">
        <a id="eealink" href="http://www.eea.europa.eu/">EEA</a>
        <a id="ewlink" href="http://ew.eea.europa.eu/">EnviroWindows</a>
      </div>

      <div id="righttools">
        <tal:block content="structure here/buttons_loginout"></tal:block>

        <a id="printlink" title="Print this page" href="javascript:this.print();">
            <span>Print</span>
        </a>

        <a id="fullscreenlink" href="javascript:toggleFullScreenMode()" title="Switch to/from full screen mode">
            <span>Switch to/from full screen mode</span>
        </a>
      </div>
    </div>

    <div id="pagehead">
      <a href="/" accesskey="1">
        <img src="/styles/eea-print-logo.gif" alt="Logo" id="logo" />
      </a>
      <div id="networktitle">Eionet</div>
      <div id="sitetitle"
           tal:content="here/site_title | nothing"></div>
      <div id="sitetagline"
           tal:content="here/site_subtitle | nothing"></div>
    </div>

    <div id="menuribbon"
         tal:content="structure here/dropdownmenus.txt"></div>
    <tal:block content="structure here/breadcrumbtrail"></tal:block>

    <div id="leftcolumn" class="localnav">
      <h2>Services</h2>
        <tal:block content="structure here/buttons_py"></tal:block>

      <h2>Account Services</h2>
      I have
      <ul>
        <li>
          <a href="http://www.eionet.europa.eu/password-reset" title="reset lost password">lost my password</a>
        </li>
      </ul>

      <div class="note-msg" style="padding: 0.5em 0.1em 0.5em 0.3em; margin: 0.2em">
        <strong style="padding: 0.5em 0.1em 0.5em 30px">Note</strong>

        <p>
          <a href="ReportekEngine/subscriptions_html">
            Subscribe to receive notifications
          </a>
          if you want to stay updated about events in this site.
        </p>
      </div>
    </div>

    <div id="workarea">
      <metal:block define-slot="content"></metal:block>
    </div>

  </div>

  <div id="pagefoot">
      Document last modified <tal:block
    content="here/bobobase_modification_time"></tal:block>.
    <span class="noprint">
      [<a href="http://validator.eea.europa.eu/check/referer">Validate HTML</a>]</span>
  </div>
</body>

</html>
