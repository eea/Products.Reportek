<dtml-var manage_page_header>
<dtml-var manage_tabs>

<h1>Edit application <dtml-var name></h1>

<form action="editApplication" method="post">

<table>
<tr>
	<th>Application</th>
	<td><dtml-var name> <input type="hidden" name="name" value="<dtml-var name>" /></td>
</tr>
<tr>
	<th>URL</th>
	<td><input size="70" type="text" name="link" value="<dtml-var link>" /></td>
</tr>
</table>

<input type="submit" value="Change" />
</form>

<dtml-var manage_page_footer>
