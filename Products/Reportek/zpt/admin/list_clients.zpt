<html>
  <head>
    <title tal:content="template/title">The title</title>
    <meta http-equiv="content-type" content="text/html;charset=utf-8">
  </head>
  <body>
    <span tal:replace="structure here/standard_html_header" />
<div class="quickjumps">
<h2>Jump to</h2>
<a href="#byperson">Sorted by person</a>
</div>
<a name="bypath"></a><h1>Clients by path</h1>
<table class="datatable">
<tr>
<th>Path</th>
<th>Last change</th>
<th>Obl</th>
<th>Client</th>
</tr>
<tr tal:repeat="hit python:here.get_results()">
	<defs tal:define="obl_hover python:here.get_obl_hover(hit)">
	<td tal:content="python:hit[0]"></td>
	<td title="title" 
		tal:attributes="title python:hit[1]" 
		tal:content="python:hit[2]"></td>
	<td tal:content="python:obl_hover[0]"></td>
	<td tal:content="python:obl_hover[1]"></td>
	</defs>
</tr>
</table>
  </body>
</html>
